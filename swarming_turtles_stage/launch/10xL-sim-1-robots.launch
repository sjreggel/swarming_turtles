    <include file="$(find swarming_turtles_stage)/launch/simulation_environment.launch" >
           <arg name="map" value="10xL_map.yaml" />
	   <arg name="stage_world" value="10xL-1-robots.world" />
    </include>

    <include file="$(find swarming_turtles_shepherd)/launch/shepherd_teleop_joy.launch" >
    </include>

    <include file="$(find swarming_turtles_stage)/launch/shepherd.launch" >
     <arg name="robot" value="robot_0" />
    </include>

    <include file="$(find swarming_turtles_stage)/launch/robot.launch" >
     <arg name="robot" value="robot_1" />
    </include>

</launch>
